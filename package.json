  },
  "homepage": "https://walker.github.io/cityworks/",
  "devDependencies": {
    "chai": "*",
    "chai-as-promised": "^7.1.1",
    "dotenv": "^10.0.0",
    "jsdoc-to-markdown": "^7.0.1",
    "microbundle": "^0.13.3",
    "mocha": "^9.1.2",
    "typedoc": "^0.22.4",
    "typescript": "^4.4.2",
    "winston": "^3.3.3"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "reversible-map": "^1.0.1"
